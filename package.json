{
  "name": "@olienttech/gx-frontend-challenges",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "postinstall": "lefthook install",
    "task": "task",
    "workspace/client": "pnpm -F @olienttech/client",
    "workspace/server": "pnpm -F @olienttech/server",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "serve": "turbo run serve",
    "format": "run-p format:*",
    "format:prettier": "prettier . -w",
    "format:eslint": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "format:stylelint": "stylelint '**/*.css' --fix",
    "lint": "run-p lint:*",
    "lint:tsc": "pnpm -F !@olienttech/tsconfig exec tsc --noEmit",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:stylelint": "stylelint '**/*.css'"
  },
  "devDependencies": {
    "@go-task/cli": "^3.32.0",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "globals": "^13.23.0",
    "jest": "^29.7.0",
    "lefthook": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "turbo": "^1.10.16",
    "typescript": "^5.0.2"
  },
  "volta": {
    "node": "20.9.0",
    "pnpm": "8.10.5"
  }
}
