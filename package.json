{
  "name": "gx-frontend-challenges",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:hcm": "hcm 'src/**/*.module.{css,scss,less}' -w",
    "build": "run-s build:hcm build:vite",
    "build:vite": "tsc && vite build",
    "build:hcm": "hcm 'src/**/*.module.{css,scss,less}'",
    "format": "run-p format:*",
    "format:eslint": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "format:stylelint": "stylelint '**/*.css' --fix",
    "format:prettier": "prettier . -w",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:stylelint": "stylelint '**/*.css'",
    "preview": "vite preview",
    "vite-bundle-visualizer": "pnpm dlx vite-bundle-visualizer",
    "storybook": "storybook dev -p 6006 --quiet --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.19.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^20.9.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "globals": "^13.23.0",
    "happy-css-modules": "^2.1.2",
    "lefthook": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "storybook": "^7.5.3",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "volta": {
    "node": "20.9.0",
    "pnpm": "8.10.5"
  }
}
